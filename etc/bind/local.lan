;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns.vert.lan. root.vert.lan. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.vert.lan.
ns      IN      A       192.168.1.100
@       IN      A       192.168.1.253
waf     IN      A       192.168.1.103
ha-proxy IN      A       192.168.1.105
server1 IN      A       192.168.1.110
server2 IN      A       192.168.1.111
redis   IN      A       192.168.1.120
pma     IN      A       192.168.1.115